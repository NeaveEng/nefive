    <child link="right_eye_ring" />
  </joint>
  <joint name="right_eye_ring_right_eye_inner_joint" type="fixed">
    <parent link="right_eye_ring" />
    <child link="right_eye_inner" />
  </joint>
  <joint name="right_eye_inner_right_camera_joint" type="fixed">
